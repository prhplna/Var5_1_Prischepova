<Window x:Class="Var5_1_Prischepova.PageAdmin.WindowLogin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Var5_1_Prischepova.PageAdmin"
        mc:Ignorable="d"
       Title="Введите код подтверждения" Height="250" Width="400">
    <Grid>
        <StackPanel Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Label Content="Введите код:" Style="{StaticResource Label}"/>
            <PasswordBox x:Name="psbPassword" Style="{StaticResource PasswordBox}"/>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                <Button x:Name="BtnBack" Content="Назад" Style="{StaticResource ButtonG}" Click="BtnBack_Click"/>
                <Button x:Name="Login" Content="Войти" Style="{StaticResource ButtonG}" Click="Login_Click"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
